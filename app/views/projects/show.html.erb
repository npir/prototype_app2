
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        
      </h1>
    </section>
  </aside>
  <div class="span10">
  	<%= link_to "Create a new Project!", newproject_path, 
                                class: "btn btn-large btn-primary" %>
		<li>
  <span class="description"><%= @project.name %></span>
  <span class="description"><%= @project.description %></span>
  

	<span class="description"><%= link_to @project.document.filename, @project.document.url if @project.document?%>


  <span class="timestamp">
    Posted <%= time_ago_in_words(@project.created_at) %> ago.
  </span>
  <% if current_user?(@project.user) %>
    <%= link_to "delete", @project, method: :delete,
                                     data: { confirm: 'You sure?' },
                                     title: @project.name %>
  <% end %>
</li>


    </div>
</div>

